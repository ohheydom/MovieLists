$("#<%= params[:movie_id] %>-tr").removeClass("movie-unwatched").addClass("movie-watched");
$("#<%= params[:movie_id] %>-form").attr({'method':"delete", 'action':"<%= escape_javascript(movie_path(params[:movie_id])) %>"});

if ($('#moviecount').length) {
  var count = $("#moviecount").html();
  $('#moviecount').text(parseInt(count)+1);
};
