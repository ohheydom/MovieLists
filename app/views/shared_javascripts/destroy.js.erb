$("#<%= params[:movie_id] %>-tr").removeClass("movie-watched").addClass("movie-unwatched");
$("#<%= params[:movie_id] %>-form").attr({'method':"post", 'action':"<%= escape_javascript(movies_path) %>"});
$("#<%= params[:movie_id] %>-tr").find("[name='-method']").attr('value', "post");

if ($('#moviecount').length) {
  var count = $("#moviecount").html();
  $('#moviecount').text(parseInt(count)-1);
};
