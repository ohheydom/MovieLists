<%= form_tag( form_data.path, id: ((movie.id).to_s + "_form"), remote: true, method: form_data.method ) do %>
  <%= hidden_field_tag :movie_id, movie.id %>
	<%= hidden_field_tag :title, movie.title %>
  <%= hidden_field_tag :release_date, movie.release_date %>
  <%= check_box_tag ((movie.id).to_s + "_button"), '', form_data.submit %>
<% end %>
