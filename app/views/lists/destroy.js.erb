$("#<%= params[:movie_id] %>_tr").removeClass("movie_watched").addClass("movie_unwatched");
$("#<%= params[:movie_id] %>_button").attr('value', "I've seen it!");
$("#<%= params[:movie_id] %>_tr").find("[name='_method']").attr('value', "post");
$("#<%= params[:movie_id] %>_form").attr({'method':"post", 'action':"<%= escape_javascript(list_path(params[:id])) %>"});
var count = $("#moviecount").html();
$('#moviecount').text(parseInt(count)-1);
